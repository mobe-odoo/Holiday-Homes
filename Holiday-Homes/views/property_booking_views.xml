<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="property_booking_tree_view" model="ir.ui.view">
        <field name="name">property.booking.tree</field>
        <field name="model">property.booking</field>
        <field name="arch" type="xml">
            <tree string="property booking">
                <field name="name_id"></field>
                <field name="phone"></field>
                <field name="total_days"></field>
                <field name="total_amount"></field>
            </tree>
        </field>
    </record>
    <record id="property_booking_form_view" model="ir.ui.view">
        <field name="name">property.booking.form</field>
        <field name="model">property.booking</field>
        <field name="arch" type="xml">
            <form string="New Booking">
                <sheet>
                    <group>
                        <group><field name="name_id"/></group>
                        <group><field name="email"/></group>
                        <group><field name="phone"/></group>
                        <group><field name="age"/></group>
                        <group><field name="members"/></group>
                        <group><field name="check_in"/></group>
                        <group><field name="check_out"/></group>
                        <group><field name="total_days"/></group>
                        <group><field name="total_amount"/></group>
                    </group> 
                </sheet>
            </form>
        </field>
    </record>
</odoo>